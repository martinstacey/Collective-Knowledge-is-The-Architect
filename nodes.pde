      if (adjpos!=null) if (adjpos.size()>0) for (int i=0; i<adjpos.size(); i++)  line (rmid.x*hsc, rmid.y*hsc, adjpos.get(i).x*hsc, adjpos.get(i).y*hsc);
      stroke(50);
      if (conpos!=null) if (conpos.size()>0) for (int i=0; i<conpos.size(); i++)  line (rmid.x*hsc, rmid.y*hsc, conpos.get(i).x*hsc, conpos.get(i).y*hsc);
    }
  }
  int compareTo (Object objI) {                                        
    Node iToCompare = (Node) objI;
    if (loc.compareTo(iToCompare.loc)<0) return -1;                              
    else if (loc.compareTo(iToCompare.loc)>0) return 1;       
    return 0;
  }
  void press() {
    if (isover()) select = !select;
    for (int i=0; i<genes.length; i++) genes[i].press();
  }
  void scroll(boolean updown) {
    for (int i=0; i<genes.length; i++) genes[i].scroll(updown);
  }
}
ArrayList<Node> setupnewnodes(String [] loc, String [] leafnames, int perm, PVector tp, PVector ts, PVector hp, PVector hs, float hsc) {
  ArrayList <Node> n = new ArrayList<Node> ();
  int nlfs = leafnames.length;
  for (int i=0; i<(nlfs+nlfs-1); i++) n.add(new Node(loc[i], tp, ts, hsc));
  n=trueleafs(n);  
  n=codeleafs(n, leafcodes, perm);
  n=leafnameandiqualities(n);
  n=setgenes(n);
  n=calchouse(n, hp, hs, hsc);
  n=fit(n);
  return n;
}
