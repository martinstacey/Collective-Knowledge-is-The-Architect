PVector locpos(String loc, PVector tp, PVector ts) {
  PVector pv;
  float pvx = tp.x;
  for (int i=1; i<loc.length(); i++) if (loc.charAt(i)=='0') pvx = pvx - ts.x/pow(2, i);        //(nodelist.get(j).pos.x-(trsize.x)/(pow(2, nodelist.get(j).gen+1))
  for (int i=1; i<loc.length(); i++) if (loc.charAt(i)=='1') pvx = pvx + ts.x/pow(2, i);
  float pvy = (((loc.length())-1) * ts.y) + tp.y; 
  pv = new PVector (pvx, pvy);
  return pv;
}
PVector fatherpos(String loc, PVector tp, PVector ts) {
  PVector fp = new PVector ();
  String floc = loc.substring(0, loc.length()-1);
  if (loc.length()==1) fp= null;
  else fp = locpos(floc, tp, ts);
  return fp;
}
ArrayList<Node> trueleafs(ArrayList<Node> _n) {
  ArrayList<Node> n = _n;
  for (Node ni : n) {
    boolean isleaf = true;
    for (Node nj : n) if (ni.loc.equals(nj.loc.substring(0, nj.loc.length()-1))) isleaf = false;
    ni.leaf = isleaf;
  }
  for (int i=0; i<n.size(); i++) n.get(i).count = i+"";  
  int leafn = 0;
  int innern = 0;
  for (Node ni : n) {
    if (ni.leaf) ni.leafcount = leafn+"";
    if (ni.leaf) leafn++;
    if (!ni.leaf) ni.innercount = innern+"";
    if (!ni.leaf) innern++;
  }
  return n;
}
ArrayList<Node> setgenes(ArrayList<Node> _n) {
  ArrayList<Node> n = _n;
  for (Node ni : n) if (ni.leaf) {
    ni.genes[0] = new Gene (int(ni.iqual[16]), 0, 1, 2, new PVector(ni.pos.x-7.5, ni.pos.y+15)); 
    ni.genes[1] = new Gene (0, 0, 1, 7, new PVector(ni.pos.x+7.5, ni.pos.y+15));
  }
  return n;
}

ArrayList<Node> codeleafs(ArrayList<Node> _n, String [] nameslf, int permutnum) {
  ArrayList<Node> n = _n;
  nameslf = permutation(nameslf, permutnum);
  int leafnum =0;
  for (Node ni : n)  if (ni.leaf) {
    if (leafcodes.length>leafnum) ni.code = nameslf [leafnum];
    leafnum++;
  } 
  return n;
}
ArrayList<Node> leafnameandiqualities(ArrayList<Node> _n) {
  ArrayList<Node> n = _n;
  for (Node ni : n) if (ni.leaf) for (int j=0; j<rprop.length; j++)  if (ni.code.equals(rprop[j][0])) {
    ni.name = rprop[j][1];
    for (int m=0; m<ni.iqual.length; m++) ni.iqual[m] = rprop[j][m];
    ni.iadj = split(rprop[j][7], ",");
    ni.icon = split(rprop[j][8], ",");
    ni.wadj = split(rprop[j][14], ",");
    ni.wcon = split(rprop[j][15], ",");
  }
  return n;
}
ArrayList<Node> calchouse(ArrayList<Node> _n, PVector hp, PVector hs, float hsc) {
  ArrayList<Node> n = _n;
  n = housesize(n, hp, hs, hsc);
  n=housequalities(n);
  n=fit(n);
  return n;
}
ArrayList<Node> housesize(ArrayList<Node> _n, PVector hp, PVector _hs, float hsc) {
  ArrayList<Node> n = _n;
  PVector hs = _hs;
  for (Node ni : n) {
    if (ni.loc.equals("0")) {
      ni.rpos = new PVector (hp.x/hsc, hp.y/hsc);
      ni.rsize =  new PVector(hs.x, hs.y);
    } else {
      Node father = ni;
      for (Node nj : n) if (nj.loc.equals(ni.loc.substring(0, ni.loc.length()-1))) father = nj;  
      int g0=int(father.genes[0].value);
      float g1 = father.genes[1].value;
      boolean c [] = {
        g0==0&&ni.child0||g0==2&&!ni.child0, 
        g0==0&&!ni.child0||g0==2&&ni.child0, 
        g0==1&&ni.child0||g0==3&&!ni.child0, 
        g0==1&&!ni.child0||g0==3&&ni.child0};
      if (c[0]||c[2]) ni.rpos = father.rpos;
      if (c[1]) ni.rpos = new PVector (father.rpos.x+(father.rsize.x*g1), father.rpos.y);
      if (c[3]) ni.rpos = new PVector (father.rpos.x, father.rpos.y+(father.rsize.y*g1));
      if (c[0]) ni.rsize = new PVector (father.rsize.x*g1, father.rsize.y);
      if (c[1]) ni.rsize = new PVector (father.rsize.x*(1-g1), father.rsize.y);
      if (c[2]) ni.rsize = new PVector (father.rsize.x, father.rsize.y*g1);
      if (c[3]) ni.rsize = new PVector (father.rsize.x, father.rsize.y*(1-g1));
    }
  }
  for (Node ni : n)  if (ni.rpos!=null&&ni.rsize!=null) ni.rmid = new PVector(ni.rpos.x+(ni.rsize.x/2.0), ni.rpos.y+(ni.rsize.y/2.0));
  return n;
}
ArrayList<Node> housequalities(ArrayList<Node> _n) {
  ArrayList<Node> n = _n;
  for (Node ni : n) if (ni.leaf) {
    if (ni.rsize.x<ni.rsize.y) {
      ni.qual[2] = nf(ni.rsize.x, 0, 2)+"";
      ni.qual[3] = nf(ni.rsize.y, 0, 2)+"";
      ni.qual[4] = nf(ni.rsize.x*ni.rsize.y, 0, 2)+"";
      ni.qual[5] = nf(ni.rsize.x/ni.rsize.x, 0, 2)+"";
    } else {
      ni.qual[2] = nf(ni.rsize.y, 0, 2)+"";
      ni.qual[3] = nf(ni.rsize.x, 0, 2)+"";
      ni.qual[4] = nf(ni.rsize.y*ni.rsize.x, 0, 2)+"";
      ni.qual[5] = nf(ni.rsize.y/ni.rsize.x, 0, 2)+"";
    }
  }
  float totarea = 0;
  for (Node ni : n) if (ni.leaf) totarea = float(ni.qual[4])+ totarea;
  for (Node ni : n) if (ni.leaf) ni.qual[6] = (float(ni.qual[4])/totarea)+""; 
  for (Node ni : n) if (ni.leaf) {
    String adjec = "";
    ArrayList <PVector> adjecpos = new ArrayList <PVector>();
    for (Node nj : n) if (nj.leaf) {
      if (ni.rpos.x+ni.rsize.x==nj.rpos.x) if (ni.rpos.y<nj.rpos.y+nj.rsize.y)  if (ni.rpos.y+ni.rsize.y>nj.rpos.y) {        
        adjec = nj.code +","+ adjec;
        adjecpos.add( nj.rmid);
      }
      if (nj.rpos.x+nj.rsize.x==ni.rpos.x) if (nj.rpos.y<ni.rpos.y+ni.rsize.y)  if (nj.rpos.y+nj.rsize.y>ni.rpos.y) {
        adjec = nj.code +","+ adjec;
        adjecpos.add( nj.rmid);
      }
      if (ni.rpos.y+ni.rsize.y==nj.rpos.y) if (ni.rpos.x<nj.rpos.x+nj.rsize.x)  if (ni.rpos.x+ni.rsize.x>nj.rpos.x) {
        adjec = nj.code +","+ adjec;
        adjecpos.add( nj.rmid);
      }
      if (nj.rpos.y+nj.rsize.y==ni.rpos.y) if (nj.rpos.x<ni.rpos.x+ni.rsize.x)  if (nj.rpos.x+nj.rsize.x>ni.rpos.x) {
        adjec = nj.code +","+ adjec;
        adjecpos.add( nj.rmid);
      }
    }
    ni.qual[7] = adjec;
    ni.adj= split(adjec, ',');
    ni.adjpos = adjecpos;
  }
  for (Node ni : n) if (ni.leaf) {
    int g1 = int(ni.genes[1].value);
    String conec = "";
    ArrayList <PVector> conecpos = new ArrayList <PVector>();
    for (Node nj : n) if (nj.leaf) {
      PVector ia = ni.rpos;
      PVector ib = new PVector (ni.rpos.x+ni.rsize.x, ni.rpos.y+ni.rsize.y);
      PVector ja = nj.rpos;
      PVector jb = new PVector (nj.rpos.x+nj.rsize.x, nj.rpos.y+nj.rsize.y);
      boolean [] d = {
        g1==0&&ia.y==jb.y, 
        g1==1&&ia.y==jb.y, 
        g1==2&&ib.x==ja.x, 
        g1==3&&ib.x==ja.x, 
        g1==4&&ib.y==ja.y, 
        g1==5&&ib.y==ja.y, 
        g1==6&&ia.x==jb.x, 
        g1==7&&ia.x==jb.x, };

      if (d[0])  if (ja.x<=ia.x&&jb.x>=ia.x+1) {
        conec = nj.code +","+ conec;  
        conecpos.add(nj.rmid);
      }
      if (d[1])  if (ja.x<=ib.x-1&&jb.x>=ib.x) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[2])  if (ja.y<=ia.y&&jb.y>=ia.y+1) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[3])  if (ja.y<=ib.y-1&&jb.y>=ib.y) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[4])  if (ja.x<=ib.x-1&&jb.x>=ib.x) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[5])  if (ja.x<=ia.x&&jb.x>=ia.x+1) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[6])  if (ja.y<=ib.y-1&&jb.y>=ib.y) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
      if (d[7])  if (ja.y<=ia.y&&jb.y>=ia.y+1) {
        conec = nj.code +","+ conec;
        conecpos.add(nj.rmid);
      }
    }
    ni.qual[8] = conec;
    ni.con= split(conec, ',');
    ni.conpos = conecpos;
  }


  return n;
}
ArrayList<Node> fit(ArrayList<Node> _n) {
  ArrayList<Node> n = _n;
  for (Node ni : n) if (ni.leaf) ni.fit = 0;
  for (Node ni : n) if (ni.leaf) {
    float fitn = 0;
    if (!ni.iqual[2].equals("")) fitn =  fitn+round((sq (float(ni.qual[2]) - float(ni.iqual[2]))* float(ni.iqual[9]))*100)/100.0;          //short * squared * weight
    if (!ni.iqual[3].equals("")) fitn =  fitn+round((sq (float(ni.qual[3]) - float(ni.iqual[3]))*float(ni.iqual[10]))*100)/100.0;         //long * squared * weight
    if (!ni.iqual[4].equals("")) fitn =  fitn+round((abs(float(ni.qual[4]) - float(ni.iqual[4]))*float(ni.iqual[11]))*100)/100.0;        //area  * weigth
    if (!ni.iqual[5].equals("")) fitn =  fitn+round((abs(float(ni.qual[5]) - float(ni.iqual[5]))*float(ni.iqual[12]))*100)/100.0;        //prop *10  * weigth
    if (ni.iadj!=null) if (ni.adj.length>0) for (int i=0; i<ni.iadj.length; i++) {
      boolean adjec = false;
      for (int j=0; j<ni.adj.length; j++) if (ni.iadj[i].equals(ni.adj[j])) adjec = true;           //ojo si no hay ninguno
      if (!adjec) fitn = (5*float(ni.wadj[i]))+fitn;
    }
    if (ni.icon!=null) if (ni.wcon!=null) for (int i=0; i<ni.icon.length; i++) {
      boolean conec = false;
      //for (int j=0; j<ni.con.length; j++)  if (ni.icon[i].equals(ni.con[j])) conec = true; 
      for (int j=0; j<ni.con.length; j++)  if (ni.icon[i].equals(ni.con[j])) {  
        if (!ni.code.equals("Cl")&&!ni.code.equals("Wc")) conec = true; 
        else {
          int g1= int(ni.genes[1].value);
          if (g1==0||g1==1||g1==4||g1==5)  if (ni.rsize.x>ni.rsize.y) conec = true; 
          if (g1==2||g1==3||g1==6||g1==7)  if (ni.rsize.x<ni.rsize.y) conec = true;
        }
      }
      if (!conec) if (ni.wcon.length>i) fitn = (25*float(ni.wcon[i]))+fitn;
    }
    ni.fit = round(fitn*100)/100.0;
  } 
  float tfit=0;
  for (Node ni : n) tfit = tfit +ni.fit;
  tfit = round(tfit*100)/100.0;
  for (Node ni : n) ni.tfit = tfit;
  return n;
}

